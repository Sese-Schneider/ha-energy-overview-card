{
  "extends": "airbnb",
  "globals": {
    "window": true,
    "Event": true,
    "customElements": true
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "indent": "off",
    "no-shadow": "off",
    "no-tabs": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off",
    "object-curly-spacing": "off",
    "quotes": "off"
  }
}
